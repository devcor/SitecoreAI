<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <model>
      <elements>
        <element interface="ContactFacets.POC.Models.IAIFacet, ContactFacets.POC" implementation="ContactFacets.POC.Models.AIFacet, ContactFacets.POC" />
      </elements>
      <entities>
        <contact>
          <facets>
            <facet name="AI" contract="ContactFacets.POC.Models.IAIFacet, ContactFacets.POC" />
          </facets>
        </contact>
      </entities>
    </model>
    <pipelines>
      <group groupName="ExperienceProfileContactDataSourceQueries">
        <pipelines>
          <ai-query>
            <processor type="ContactFacets.POC.Pipelines.AIQuery, ContactFacets.POC" />
          </ai-query>
        </pipelines>
      </group>
      <group groupName="ExperienceProfileContactViews">
        <pipelines>
          <ai>
            <processor type="ContactFacets.POC.Pipelines.AITableViewDefinition, ContactFacets.POC" />
            <processor type="Sitecore.Cintel.Reporting.Processors.ExecuteReportingServerDatasourceQuery, Sitecore.Cintel">
              <param desc="queryName">ai-query</param>
            </processor>
            <processor type="ContactFacets.POC.Pipelines.AITableViewFiller, ContactFacets.POC" />
          </ai>
        </pipelines>
      </group>
    </pipelines>
  </sitecore>
</configuration>
